# Configurações do Banco de Dados PostgreSQL (Local)
# Preencha estas variáveis no seu arquivo .env pessoal
DB_HOST=127.0.0.1
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=SUA_SENHA_LOCAL_AQUI
DB_NAME=carona_api

# Configurações de Autenticação JWT
# Gere um segredo forte e aleatório para o seu .env pessoal
JWT_SECRET=GERAR_UM_SEGREDO_FORTE_E_ALEATORIO_AQUI
JWT_EXPIRES_IN=1d